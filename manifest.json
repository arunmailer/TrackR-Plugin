{
  "name": "A plugin to track and update tweets",
  "description": "Changes the tweets selectively",
  "version": "1.0",
  "permissions": [
    "tabs", "http://twitter.com/", "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://twitter.com/", "https://twitter.com/*"],
      "js": ["script.js"]
    }
  ],
  "manifest_version": 2
}
